{
  "name": "powpow",
  "version": "0.0.0",
  "description": "Multiplayer Shooter game based on Node and WebSockets",
  "main": "src/server/js/index.js",
  "scripts": {
    "start": "NODE_ENV=production && node src/server/js/index.js",
    "start:dev": "NODE_ENV=develop && nodemon --ignore node_modules --ignore src/client src/server/js/index.js"
  },
  "keywords": [
    "shooter",
    "game",
    "multiplayer",
    "websockets"
  ],
  "engines": {
    "node": ">=5.10.1"
  },
  "author": "@3rfan",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "config": "^1.20.1",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^1.1.0",
    "nodemon": "^1.9.1"
  },
  "dependencies": {
    "express": "^4.13.4",
    "jquery": "^2.2.3",
    "phaser": "^2.4.6",
    "socketio": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "jshintConfig": {
    "globals": {
      "$": false,
      "_": false
    }
  }
}
