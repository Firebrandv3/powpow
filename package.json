{
  "name": "powpow",
  "version": "0.0.0",
  "description": "Multiplayer Shooter game based on Node and WebSockets",
  "main": "src/server/js/index.js",
  "scripts": {
    "start": "NODE_ENV=production && node src/server/js/index.js",
    "start:dev": "NODE_ENV=develop && nodemon --ignore node_modules --ignore src/client src/server/js/index.js"
  },
  "keywords": [
    "shooter",
    "game",
    "multiplayer",
    "websockets"
  ],
  "engines": {
    "node": ">=5.10.1"
  },
  "author": "@3rfan",
  "license": "ISC",
  "devDependencies": {
    "bootstrap-grid": "git+https://github.com/scriptheroes/bootstrap-grid.git#v1.0.1",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "config": "^1.20.1",
    "config-browserify": "^1.0.5",
    "grunt": "^1.0.1",
    "grunt-banner": "^0.6.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^1.1.0",
    "nodemon": "^1.9.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "jquery": "^2.2.3",
    "moment": "^2.13.0",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pg": "^4.5.5",
    "pg-hstore": "^2.3.2",
    "phaser": "^2.4.6",
    "sequelize": "^3.23.2",
    "sequelize-connect": "^1.2.2",
    "socketio": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "jshintConfig": {
    "globals": {
      "$": false,
      "_": false,
      "require": false,
      "Phaser": false
    }
  }
}
